function sc_glyph_bound(glyph_id){
	if(glyph_id = ref_glyph_mist or glyph_id = ref_glyph_transform or glyph_id = ref_glyph_sketch) or glyph_id = ref_glyph_command or 
	glyph_id = ref_glyph_determination or glyph_id = ref_glyph_rush or glyph_id = ref_glyph_taunt{
		return true;
	}
	return false;
}